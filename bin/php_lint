#!/bin/bash

# shellcheck source=./lib/base_command
source "$(dirname "${BASH_SOURCE[0]}")/lib/base_command"

docker-compose-exec-bash php "./vendor/bin/php-cs-fixer fix --using-cache=no --verbose"
docker-compose-exec-bash php "vendor/bin/phpstan analyse src tests --memory-limit=512M"
docker-compose-exec-bash php "vendor/bin/psalm --no-cache"
